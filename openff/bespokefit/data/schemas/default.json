  "default_qc_specs": [
    {
      "method": "B3LYP-D3BJ",
      "basis": "DZVP",
      "program": "psi4",
      "spec_name": "default",
      "spec_description": "Standard OpenFF optimization quantum chemistry specification.",
      "store_wavefunction": "none",
      "implicit_solvent": null,
      "maxiter": 200,
      "scf_properties": [
        "dipole",
        "quadrupole",
        "wiberg_lowdin_indices",
        "mayer_indices"
      ],
      "keywords": null
    }
